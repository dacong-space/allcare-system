{
  "name": "allcare-system",
  "private": true,
  "version": "1.1.5",
  "type": "module",
  "homepage": "https://dacong-space.github.io/allcare-system",
  "scripts": {
    "dev": "vite",
    "prebuild": "node update-version.cjs",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "version:patch": "node version-bump.js patch",
    "version:minor": "node version-bump.js minor",
    "version:major": "node version-bump.js major",
    "prepare": "husky install",
    "e2e": "cypress open",
    "e2e:ci": "cypress run --headless"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "antd": "^5.24.6",
    "axios": "^1.8.4",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "moment": "^2.30.1",
    "react": "^18.3.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.3.1",
    "react-markdown": "^10.1.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^7.5.0",
    "styled-components": "^6.1.17"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@types/cypress": "^12.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "gh-pages": "^6.3.0",
    "globals": "^15.15.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "vite": "^6.2.0",
    "cypress": "^12.17.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
